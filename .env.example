 Copy this to .env.local and add your API keys

# Google Custom Search API (for comprehensive blog discovery)
GOOGLE_SEARCH_API_KEY=your_google_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here

# Bing Search API (backup search engine)
BING_SEARCH_API_KEY=your_bing_api_key_here

# Optional: Claude API for advanced contact extraction
CLAUDE_API_KEY=your_claude_api_key_here

# Database URL (defaults to local SQLite)
DATABASE_URL=./typepad-rescue.db

# Rate limiting (requests per minute)
SEARCH_RATE_LIMIT=60
SCRAPE_RATE_LIMIT=30
